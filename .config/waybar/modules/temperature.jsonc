{
  "temperature": {
    "thermal-zone": 1,
    "interval": 10, //default 10
    "min-length": 10,
    "max-length": 10,
    "warning-threshold": 82,
    "critical-threshold": 90,
    "format": "{icon}  {temperatureC}°C",
    "format-icons": [
      "", "", "", "", ""
    ],
    "tooltip": false,
    "on-click": "hyprctl dispatch exec 'ghostty -e btop'"
  }
}
